{
  "compilerOptions": {
    "module": "es2022",
    "moduleResolution": "node",
    "target": "ES2022",
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": "src",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
   "include": ["src/**/*.ts", "src/index.ts"],
}